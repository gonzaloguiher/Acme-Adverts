<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd">

	<util:list id="converters">
			<bean class="converters.AuthorityToStringConverter" />
			<bean class="converters.AdministratorToStringConverter" />
			<bean class="converters.ContractToStringConverter" />
			<bean class="converters.CustomerToStringConverter" />
			<bean class="converters.BillboardFileToStringConverter"  />
			<bean class="converters.RadioFileToStringConverter"  />
			<bean class="converters.TVFileToStringConverter"     />
			<bean class="converters.InfoFileToStringConverter"   />
			<bean class="converters.SocialNetworkFileToStringConverter" />
			<bean class="converters.ActorToStringConverter" />
			<bean class="converters.FinderToStringConverter"  />
			<bean class="converters.ManagerToStringConverter" />
			<bean class="converters.PackageToStringConverter" />
			<bean class="converters.RequestToStringConverter" />
			
			<bean class="converters.StringToAuthorityConverter" />
			<bean class="converters.StringToAdministratorConverter" />
			<bean class="converters.StringToContractConverter" />
			<bean class="converters.StringToCustomerConverter" />
			<bean class="converters.StringToBillboardFileConverter" />
			<bean class="converters.StringToRadioFileConverter"  />
			<bean class="converters.StringToTVFileConverter"     />
			<bean class="converters.StringToInfoFileConverter"   />
			<bean class="converters.StringToSocialNetworkFileConverter" />
			<bean class="converters.StringToActorConverter" />
			<bean class="converters.StringToFinderConverter"  />
			<bean class="converters.StringToManagerConverter" />
			<bean class="converters.StringToPackageConverter" />
			<bean class="converters.StringToRequestConverter" />
	</util:list>
	
</beans>